---
interface Props {
    price: number;
}

const { price } = Astro.props;
---

<div class="square">
    <span>{price}</span>
    <slot />
</div>

<style>
    div {
        display: flex;
        border: 1px solid #000;
        flex: 1;
        flex-direction: column;
        align-items: center;
        justify-content: space-around;
    }
</style>
